{
  "name": "shell",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm build:all",
    "build:all": "rsbuild build --environment client_ssr --environment client_ssr_server --environment admin_csr",
    "build:client": "rsbuild build --environment client_ssr --environment client_ssr_server",
    "build:admin": "rsbuild build --environment admin_csr",
    "dev": "pnpm dev:all",
    "dev:all": "concurrently \"pnpm dev:client\" \"pnpm dev:admin\"",
    "dev:client": "rsbuild dev --environment client_ssr",
    "dev:admin": "rsbuild dev --environment admin_csr",
    "start": "node server.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "pnpm build && pnpm start"
  },
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router-dom": "7.9.3",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@rsbuild/core": "1.5.12",
    "@rsbuild/plugin-react": "1.4.1",
    "@rsbuild/plugin-node-polyfill": "^1.0.4",
    "@types/express": "^5.0.6",
    "concurrently": "^9.2.1",
    "@types/react": "19.1.15",
    "@types/react-dom": "19.1.9",
    "eslint": "9.36.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.22",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "@module-federation/enhanced": "0.21.6",
    "@module-federation/rsbuild-plugin": "0.21.6"
  }
}
